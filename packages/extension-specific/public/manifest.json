{
  "name": "Next JS Chrome Extension",
  "description": "Next.js Chrome Extension starter",
  "manifest_version": 3,
  "action": {},
  "content_scripts": [
    {
      "matches": ["<all_urls>"],
      "js": ["content.js"]
    }
  ],
  "background": {
    "service_worker": "background.js"
  },
  "permissions": ["activeTab", "scripting"]
}
